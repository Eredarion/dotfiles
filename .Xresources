!-- URxvt -------------------------------------------------------------------------
! URxvt #101423  #070D1F  
URxvt*foreground: #ECEFF4
URxvt*background: [100]#20242b
URxvt*saveline: 15000
URxvt*termName: rxvt-256color
URxvt*iso14755: false
URxvt*depth: 32
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*internalBorder: 5
URxvt*externalBorder: 0
*URxvt.geometry: 68x18
URxvt.iconFile: /home/ranguel/Downloads/terminal-icon_34340.png

!-- Font -------------------:style=Medium
!URxvt.letterSpace: -1
#define termfont  xft:Iosevka Term:pixelsize=14:antialias=true:autohint=true
!#define termfont  xft:Iosevka:pixelsize=14:antialias=true:autohint=true
URxvt.font: termfont
URxvt.boldFont: termfont:style=Medium
URxvt.italicFont: termfont:style=italic
URxvt.boldItalicFont: termfont:style=bolditalic
!---------------------------

!-- Font config ------------
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: none
Xft.hinting: true
Xft.autohint: true
Xft.hintstyle: hintslight
!---------------------------

!-- Keys -------------------
URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.keysym.M-v: eval:paste_clipboard
URxvt.keysym.M-c: eval:selection_to_clipboard
URxvt.keysym.M-V: eval:paste_clipboard
URxvt.keysym.M-C: eval:selection_to_clipboard
!---------------------------

URxvt.urlLauncher: exo-open
URxvt.underlineURLs: true
URxvt.urlButton: 1
URxvt*buffered: false
!----------------------------------------------------------------------------------


