URxvt*internalBorder: 15
URxvt*externalBorder: 0
*URxvt.geometry: 54x16

*.font : FantasqueSansMono Nerd Font:pixelsize=15:antialias=true:autohint=true
!*.font : Iosevka:pixelsize=14:antialias=true:autohint=true
*.cwscale : 0.9
*.chscale : 1.0

! URxvt #101423  #070D1F  
!URxvt*foreground: #EAE1EA
!URxvt*background: [100]#101423
URxvt*saveline: 15000
URxvt*termName: rxvt-256color
URxvt*iso14755: false
URxvt*depth: 32
URxvt*scrollBar: false
URxvt*scrollBar_right: false

!URxvt.letterSpace: -1
!URxvt*letterSpace: 1
!URxvt.font: xft:DejaVu Sans Mono:size=11 
#define termfont  xft:FantasqueSansMono Nerd Font:pixelsize=14:antialias=true:autohint=true
!#define termfont      xft:mplus Nerd Font:size=8:antialias=true
URxvt.font: termfont
URxvt.boldFont: termfont:style=Bold
URxvt.italicFont: termfont:style=italic
URxvt.boldItalicFont: termfont:style=bolditalic

Xft.dpi: 96
Xft.antialias: true
Xft.rgba: none
Xft.hinting: true
Xft.autohint: true
Xft.hintstyle: hintslight

URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.urlLauncher: exo-open
URxvt.underlineURLs: true
URxvt.urlButton: 1
URxvt*buffered: false


